include .envrc

.PHONY: run/api
run/api:
	@echo 'Running Product API...'
	@go run ./cmd/api -port=3000 -env=production -db-dsn=${PRODUCTS_DB_DSN}

.PHONY: db/psql
db/psql:
	psql ${PRODUCTS_DB_DSN}

.PHONY: db/migrations/new
db/migrations/new:
	@echo 'Creating migration files for ${name}...'
	migrate create -seq -ext=.sql -dir=./migrations ${name}

.PHONY: db/migrations/up
db/migrations/up:
	@echo 'Running up migrations...'
	migrate -path=./migrations -database ${PRODUCTS_DB_DSN} up

.PHONY: displayProduct
displayProduct:
	@echo 'Displaying Product'; \
	curl -i localhost:3000/displayProduct/${id} 


.PHONY: createProduct
createProduct:
	@echo 'Creating Product'; \
            BODY='{"name":"Chainsaw", "description":"Good for Wood", "category":"Power Tools", "image_url":"data:image/jpeg;base64,", "price":2.10}'; \
	echo "$$BODY"; \
	curl -i -d "$$BODY" localhost:3000/createProduct ; \
	echo 'create a product'
.PHONY: a_all
a_all:
	@echo 'Creating Product'; \
	BODY='{"name":"Standard Broom", "description":"A sturdy broom designed for indoor and outdoor use, perfect for sweeping up dirt and debris.", "category":"Cleaning Supplies", "image_url":"data:image/jpeg;base64,"}'; \
	echo "$$BODY"; \
	curl -i -d "$$BODY" localhost:3000/createProduct ; \
	BODY='{"name":"Soap", "description":"Good For Cleaining", "category":"Cosmetics", "image_url":"data:image/jpeg;base64,"}'; \
	echo "$$BODY"; \
	curl -i -d "$$BODY" localhost:3000/createProduct ; \



